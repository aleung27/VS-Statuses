{
	"name": "vstatus",
	"displayName": "Vstatus",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vstatus.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "vstatus.update",
				"title": "Send updates",
				"category": "Vstatus"
			},
			{
				"command": "vstatus.stopUpdate",
				"title": "Stop updates",
				"category": "Vstatus"
			},
			{
				"command": "vstatus.auth",
				"title": "Authenticate with Github",
				"category": "Vstatus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.19.11",
		"@types/vscode": "^1.52.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"https": "^1.0.0",
		"jsdoc": "^3.6.6"
	}
}
